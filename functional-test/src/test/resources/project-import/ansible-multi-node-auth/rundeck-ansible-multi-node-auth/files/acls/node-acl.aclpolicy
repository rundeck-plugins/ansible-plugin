description: Node access for project
context:
  project: '.*'
for:
  resource:
    - equals:
        kind: node
      allow: [read,create,update,refresh]
  adhoc:
    - allow: [read,run,runAs,kill,killAs]
  job:
    - allow: [create,read,update,delete,run,runAs,kill,killAs]
  node:
    - allow: [read,run]
by:
  username: admin
